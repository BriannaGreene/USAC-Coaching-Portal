<head>
  <!-- Required meta tags -->
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

  <link rel="icon" href="/favicon.ico" type="image/x-icon">
  <!-- bootstrap4 -->
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-beta/css/bootstrap.min.css" integrity="sha384-/Y6pD6FV/Vv2HJnA6t+vslU6fwYXjCFtcEpHbNJ0lyAFsXTsjBbfaDjzALeQsN6M" crossorigin="anonymous">
  <!-- bootstrap3 -->
  <!-- <link href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" rel="stylesheet"> -->
  <!-- personal style -->
  <link rel="stylesheet" href="/stylesheets/style.css">

  <title>USAC Coaching Portal</title>
</head>

<body>

  <nav id="mainnav" class="navbar-inverse">
      <div id="logo-div" class="navbar-header">
        <a class="navbar-brand" href="/"><img id="logo-image" src="img/usac_sec_c.png" alt="USAC Coaching Portal"></a>
        <div id="login-form">
          <h2 id="head-text">USA CLIMBING COACHES PORTAL</h2>
          <form class="form-inline mt-2 mt-md-0">
            <input id="login-email" class="form-control" type="text" placeholder="Email" aria-label="Email" required>
            <input id="login-pass" class="form-control mr-sm-2" type="password" placeholder="Password" aria-label="Password" required>
            <button id="login-button" class="btn btn-outline-success my-2 my-sm-0" type="submit">LOGIN</button>
          </form>
        </div>
      </div>
      <div id="login-form" class="collapse navbar-collapse">
        <h2 id="head-text">USA CLIMBING COACHES PORTAL</h2>
        <form class="form-inline mt-2 mt-md-0">
          <input id="login-email" class="form-control mr-sm-2" type="text" placeholder="Email" aria-label="Email" required>
          <input id="login-pass" class="form-control mr-sm-2" type="password" placeholder="Password" aria-label="Password" required>
          <button id="login-button" class="btn btn-outline-success my-2 my-sm-0" type="submit">LOGIN</button>
        </form>
      </div>
  </nav>

  <div class="container-fluid content">

    <%- body -%>

  </div>

  <footer class="footer-overwrite">
    <div class="container-fluid">
      <div class="row">
        <div class="col-md-3">
          <img id="footer-logo" src="/img/usac_logo_white.png" alt="USA Climbing">
          <p>4665 Nautilus Ct S #502A</br>
          Boulder, CO 80301</br>
          303-499-0715</br>
          <a href="#">info@usaclimbing.org</a></p>
        </div>
        <div id="footer-right" class="col-m-9 align-self-center">
          <img id="tnf-logo" src="/img/tnf_square.jpg" alt="">
        </div>
      </div>
      <div class="row">
        <div class="col-md-8">
          <img class="footer-img" src="/img/adaptive.png" alt="">
          <img class="footer-img" src="/img/bouldering.png" alt="">
          <img class="footer-img" src="/img/collegiate.png" alt="">
          <img class="footer-img" src="/img/speed.png" alt="">
          <img class="footer-img" src="/img/sport.png" alt="">
        </div>
        <div class="col-md-4">
          <img class="social-logo" src="/img/fb-wh.png" alt="">
          <img class="social-logo" src="/img/tw-wh.png" alt="">
          <img class="social-logo" src="/img/in-wh.png" alt="">
          <img class="social-logo" src="/img/yt-wh.png" alt="">
        </div>

      </div>
    </div>
  </footer>

  <!-- Optional JavaScript -->
  <script src="https://code.jquery.com/jquery-3.2.1.min.js" integrity="sha256-hwg4gsxgFZhOsEEamdOYGBf13FyQuiTwlAQgxVSNgt4=" crossorigin="anonymous"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.11.0/umd/popper.min.js" integrity="sha384-b/U6ypiBEHpOf/4+1nzFpr53nxSS+GLCkfwBdFNTxtclqqenISfwAzpKaMNFNmj4" crossorigin="anonymous"></script>
  <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-beta/js/bootstrap.min.js" integrity="sha384-h0AbiXch4ZDo7tp9hKZ4TsHbi047NrKGLO3SEJAg45jXxnGIfYzk4Si90RDIqNm1" crossorigin="anonymous"></script>

  <script src="/javascripts/admin.js" charset="utf-8"></script>
  <script src="/javascripts/coaches.js" charset="utf-8"></script>
  <script src="/javascripts/login.js" charset="utf-8"></script>
  <script src="/javascripts/users.js" charset="utf-8"></script>
</body>

</html>
