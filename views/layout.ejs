<head>
  <!-- Required meta tags -->
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

  <link rel="icon" href="/favicon.ico" type="image/x-icon">
  <link href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" rel="stylesheet">
  <link rel="stylesheet" href="/stylesheets/style.css">
  <title>USAC Coaching Portal</title>
</head>

<body>

  <nav id="mainnav" class="navbar-inverse">
    <div class="container">
      <div id="logo-div" class="navbar-header">
        <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
          <span class="sr-only">Toggle navigation</span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
        </button>
        <a class="navbar-brand" href="/"><img id="logo-image" src="img/usac_logo_white.png" alt="USAC Coaching Portal"></a>
        <a id="logo-words" class="navbar-brand" href="/"><h2>COACHES PORTAL</h2></a>
      </div>
      <div id="login-form" class="collapse navbar-collapse">
        <form class="form-inline mt-2 mt-md-0">
          <input id="login-email" class="form-control mr-sm-2" type="text" placeholder="Email" aria-label="Email" required>
          <input id="login-pass" class="form-control mr-sm-2" type="password" placeholder="Password" aria-label="Password" required>
          <button id="login-button" class="btn btn-outline-success my-2 my-sm-0" type="submit">Login</button>
        </form>
      </div>
    </div>
  </nav>

  <div class="container-fluid content">

    <%- body -%>

  </div>

  <footer class="footer-overwrite">
    <div class="container">
      <span class="text-muted">Certified Coaches List</span>
    </div>
  </footer>

  <!-- Optional JavaScript -->
  <script src="https://code.jquery.com/jquery-3.2.1.min.js" integrity="sha256-hwg4gsxgFZhOsEEamdOYGBf13FyQuiTwlAQgxVSNgt4=" crossorigin="anonymous"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.11.0/umd/popper.min.js" integrity="sha384-b/U6ypiBEHpOf/4+1nzFpr53nxSS+GLCkfwBdFNTxtclqqenISfwAzpKaMNFNmj4" crossorigin="anonymous"></script>
  <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-beta/js/bootstrap.min.js" integrity="sha384-h0AbiXch4ZDo7tp9hKZ4TsHbi047NrKGLO3SEJAg45jXxnGIfYzk4Si90RDIqNm1" crossorigin="anonymous"></script>
  <!-- CLOUDINARY SCRIPTS -->
  <script src='https://cdn.jsdelivr.net/jquery.cloudinary/1.0.18/jquery.cloudinary.js' type='text/javascript'></script>
  <script src="https://widget.cloudinary.com/global/all.js" type="text/javascript"></script>
  <!-- <script src="/script.js"></script> -->
  <!-- CLOUDINARY SCRIPTS -->
  <script src="/javascripts/admin.js" charset="utf-8"></script>
  <script src="/javascripts/coaches.js" charset="utf-8"></script>
  <script src="/javascripts/login.js" charset="utf-8"></script>
  <script src="/javascripts/users.js" charset="utf-8"></script>
  <script src="/javascripts/jwt.js"></script>
</body>

</html>
